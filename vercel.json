{
     "routes": [
         {
             "src": "/api/(.*)",
             "dest": "/api/$1",
             "status": 200
         },
         {
             "src": "/static/(.*)",
             "dest": "/static/$1",
             "status": 200
         },
         {
             "src": "/[^.]+",
             "dest": "/",
             "status": 200
         }
     ]
 }
 